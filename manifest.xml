<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <Id>6ac6aee7-5b3b-4d01-9df2-3c9a31e25a5f</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>ORF TOOL</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="ORF TOOL"/>
  <Description DefaultValue="A template to get started with ORF TOOL."/>
  <IconUrl DefaultValue="http://codewithabubakr.github.io/ORF-Addin/assets/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="http://codewithabubakr.github.io/ORF-Addin/assets/icon-64.png"/>
  <SupportUrl DefaultValue="https://www.contoso.com/help"/>
  <AppDomains>
    <AppDomain>https://www.contoso.com</AppDomain>
    <AppDomain>https://localhost:3000</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Workbook"/>
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="http://codewithabubakr.github.io/ORF-Addin/taskpane.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <Runtimes>
    <Runtime resid="Functions.Url" lifetime="long"/>
  </Runtimes>
  <ExtensionPoint xsi:type="CustomFunctions">
    <Script>
      <SourceLocation DefaultValue="http://codewithabubakr.github.io/ORF-Addin/dist/functions.js"/>
    </Script>
    <Page>
      <SourceLocation DefaultValue="http://codewithabubakr.github.io/ORF-Addin/dist/functions.html"/>
    </Page>
    <Metadata>
      <SourceLocation DefaultValue="http://codewithabubakr.github.io/ORF-Addin/dist/functions.json"/>
    </Metadata>
    <Namespace resid="Functions.Namespace"/>
  </ExtensionPoint>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Workbook">
        <Runtimes>
          <Runtime resid="Taskpane.Url" lifetime="long"/>
          <Runtime resid="Functions.Url" lifetime="long"/>
        </Runtimes>
        <DesktopFormFactor>
          <GetStarted>
            <Title resid="GetStarted.Title"/>
            <Description resid="GetStarted.Description"/>
            <LearnMoreUrl resid="GetStarted.LearnMoreUrl"/>
          </GetStarted>
          <FunctionFile resid="Functions.Url"/>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <OfficeTab id="TabHome">
              <Group id="CommandsGroup">
                <Label resid="CommandsGroup.Label"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
                <Control xsi:type="Button" id="TaskpaneButton">
                  <Label resid="TaskpaneButton.Label"/>
                  <Supertip>
                    <Title resid="TaskpaneButton.Label"/>
                    <Description resid="TaskpaneButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>ButtonId1</TaskpaneId>
                    <SourceLocation resid="Taskpane.Url"/>
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="Icon.16x16" DefaultValue="http://codewithabubakr.github.io/ORF-Addin/assets/icon-16.png"/>
        <bt:Image id="Icon.32x32" DefaultValue="http://codewithabubakr.github.io/ORF-Addin/assets/icon-32.png"/>
        <bt:Image id="Icon.80x80" DefaultValue="http://codewithabubakr.github.io/ORF-Addin/assets/icon-80.png"/>
      </bt:Images>
      <bt:Urls>
        <bt:Url id="GetStarted.LearnMoreUrl" DefaultValue="https://go.microsoft.com/fwlink/?LinkId=276812"/>
        <bt:Url id="Commands.Url" DefaultValue="http://codewithabubakr.github.io/ORF-Addin/commands.html"/>
        <bt:Url id="Taskpane.Url" DefaultValue="http://codewithabubakr.github.io/ORF-Addin/taskpane.html"/>
        <bt:Url id="Functions.Url" DefaultValue="http://codewithabubakr.github.io/ORF-Addin/dist/functions.html"/>
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="GetStarted.Title" DefaultValue="Get started with ORF TOOL!"/>
        <bt:String id="CommandsGroup.Label" DefaultValue="Add-ins"/>
        <bt:String id="TaskpaneButton.Label" DefaultValue="ORF TOOL"/>
        <bt:String id="Functions.Namespace" DefaultValue="MYADDIN"/>
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="GetStarted.Description" DefaultValue="Your ORF TOOL add-in loaded successfully. Go to the HOME tab and click the 'ORF TOOL' button to get started."/>
        <bt:String id="TaskpaneButton.Tooltip" DefaultValue="Click to Show a ORF TOOL."/>
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>